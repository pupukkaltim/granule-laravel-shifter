{
    "name": "granule/laravel-shifter",
    "description": "Shift your Laravel project that integrated with Granule Starter Kit version to the newer version",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Granule\\LaravelShifter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sallieeky",
            "email": "sallieeky@gmail.com"
        },
        {
            "name": "sallixie",
            "email": "10191077@student.itk.ac.id"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/console": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "laravel": {
            "providers": [
                "Granule\\LaravelShifter\\StarterKitServiceProvider"
            ]
        }
    }
}
